table{background-color: white;margin-top: 8px;}
.mui-card-header:after {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    height: 1px;
    content: '';
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    background-color: white;
}
/*.mui-media-body{font-size: 18px;}*/
